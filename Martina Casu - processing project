        <script type="text/processing">


size(400, 400);
              background(100, 100, 50);
            }
            void draw(){
                pushMatrix();
                fill(50, 60, 10);
                translate(200, 200);
                rotate(radians(frameCount * 2  % 360));
                quad(50, 31, 45, 20, 69, 63, 30, 76);
                popMatrix();
           
           
                pushMatrix();
                fill(100, 0, 10);
                translate(200, 200);
                rotate(radians(frameCount * 2  % 360));
                rect(0, 0, 45, 40);
                popMatrix();
            }
            
             </script>
        <canvas></canvas>
